<div class="actions">
  <%= link_to 'Back', students_path %> |
  <%= link_to 'Edit', edit_student_path(@student) %>
</div>

<p id="notice"><%= notice %></p>

<p>
  <strong>First name:</strong>
  <%= @student.first_name %>
</p>

<p>
  <strong>Last name:</strong>
  <%= @student.last_name %>
</p>

<p>
  <strong>Ugid:</strong>
  <%= @student.ugid %>
</p>

<p>
	<strong>Roles:</strong>
</p>
	<% @student.roles.each do |role| %>
		<p>
			<%= role.name %>
		</p>
	<% end -%>
<p>
	<strong>
		Positions:
	</strong>
</p>
<p>
	<% @student.positions.each do |position| %>
		<p>
			<%= position.name %>
		</p>
	<% end -%>
</p>
<p>
	<strong>
		Privileges:
	</strong>
</p>
<p>
	<% @student.privileges.each do |privilege| %>
		<p>
			<%= privilege.name %>
		</p>
	<% end -%>
</p>